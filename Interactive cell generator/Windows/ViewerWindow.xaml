<Window x:Class="Interactive_cell_generator.Windows.ViewerWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Interactive_cell_generator.Windows"
        xmlns:h="http://helix-toolkit.org/wpf"
        mc:Ignorable="d"
        Title="ViewerWindow" Height="450" Width="800">
    <Grid>
        <h:HelixViewport3D    x:Name="viewport"    ZoomExtentsWhenLoaded="True">            <h:HelixViewport3D.Camera>                <PerspectiveCamera            FieldOfView="45"            LookDirection="0,0,-10"            Position="0,0,10"            UpDirection="0,1,0" />            </h:HelixViewport3D.Camera>            <ModelVisual3D Content="{Binding Models}" />            <h:SunLight />        </h:HelixViewport3D>
    </Grid>
</Window>
